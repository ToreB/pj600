<?xml version="1.0"?>
<configuration>

  <!-- Config for Register.aspx -->
  <location path="Register.aspx">
    <system.web>
      <authorization>
        <allow roles="Admin"/>
        <deny users="*"/>
      </authorization>
    </system.web>
  </location>

   <!-- Config for ResetPassword.aspx -->
   <location path="ResetPassword.aspx">
      <system.web>
         <authorization>
            <allow users="?"/>
         </authorization>
      </system.web>
   </location>

</configuration>
